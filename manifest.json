{
  "manifest_version": 3,
  "name": "Tree Swipe",
  "version": "0.02",
  "description": "A Chrome extension designed to help people quickly label and sort their email inboxes",
  "permissions": [
    "tabs",
    "storage",
    "identity",
    "activeTab"
  ],
  "host_permissions": [
    "https://mail.google.com/"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "oauth2": {
    "client_id": "626617498536-r6rqg468873qp8589t4795c99p570ooq.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/gmail.modify",
      "https://www.googleapis.com/auth/gmail.labels"
    ]
  }
}